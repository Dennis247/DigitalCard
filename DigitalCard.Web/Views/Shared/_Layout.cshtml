@using Newtonsoft.Json

@inject IHttpContextAccessor HttpContextAccessor
 AuthenticateResponse account = null;
@{
    Customer account = new Customer
    {
        FirstName = "Dennis",
        LastName = "",
        Email = "",
    };

    string userdData = HttpContextAccessor.HttpContext.Session.GetString("userProfile");
    if(userdData != null)
    {
      account = JsonConvert.DeserializeObject<Customer>(userdData);
    }

 }



